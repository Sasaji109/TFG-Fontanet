type Equipo {
    id: Int!
    nombre: String
    jugadores: [Jugador]
}

type Jugador {
    nombre: String
    equipoId : Int
}

type Query {
    getEquipos: [Equipo]!
    getEquipoById(id: Int!): Equipo
}

type Mutation {
    addJugadorToEquipo(nuevoJugador: JugadorInput): Int
}

input JugadorInput {
    nombre: String
    equipoId: Int
}