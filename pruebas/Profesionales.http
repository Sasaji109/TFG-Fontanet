###
# @name getProfesionales
GRAPHQL http://localhost:8080/graphql

query getProfesionales {
  getAllProfesionales {
    profesionalId
    nombre
    apellidos
    numero
    experiencia
    oficio
    disponibilidad
    valoracion
  }
}

